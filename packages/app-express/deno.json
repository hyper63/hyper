{
  "name": "@hyper63/app-express",
  "version": "1.2.1",
  "exports": "./mod.ts",
  "tasks": {
    "test": "deno lint && deno fmt --check && deno test -A --no-check",
    "cache": "deno install --entrypoint mod.ts",
    "harness": "deno run --no-check --no-lock -A --reload ./harness.ts"
  },
  "imports": {
    "express": "npm:express@4.18.2",
    "multer": "npm:multer@1.4.4",
    "body-parser": "npm:body-parser@1.20.2",
    "cors": "npm:cors@2.8.5",
    "helmet": "npm:helmet@7.1.0",
    "ramda": "npm:ramda@0.29.1",
    "crocks": "npm:crocks@0.12.4",
    "@hyper63/utils": "jsr:@hyper63/utils@0.1.2/mod.js",
    "@hyper63/core": "jsr:@hyper63/core@4.3.3",
    "@std/assert": "jsr:@std/assert@1",
    "@std/media-types": "jsr:@std/media-types@1",
    "@std/dotenv": "jsr:@std/dotenv@0.225.2",
    "@std/path": "jsr:@std/path@1"
  },
  "fmt": {
    "include": [
      "./"
    ],
    "lineWidth": 100,
    "singleQuote": true,
    "semiColons": false
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-slow-types"
      ]
    }
  },
  "publish": {
    "include": [
      "./*",
      "./api/**/*",
      "./middleware/**/*"
    ],
    "exclude": [
      "**/*.test.ts",
      "harness.ts"
    ]
  }
}
