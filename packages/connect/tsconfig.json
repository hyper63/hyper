{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": false
  },
  "include": [
    "deno"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
