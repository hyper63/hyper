{
  "name": "@hyper63/port-data",
  "version": "2.3.0",
  "exports": "./mod.ts",
  "tasks": {
    "test": "deno lint && deno fmt --check && deno test --no-check",
    "cache": "deno install --entrypoint mod.ts"
  },
  "imports": {
    "zod": "npm:zod@3.20.5",
    "@std/assert": "jsr:@std/assert@1"
  },
  "fmt": {
    "include": [
      "./"
    ],
    "lineWidth": 100,
    "singleQuote": true,
    "semiColons": false
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-slow-types"
      ]
    }
  },
  "publish": {
    "include": [
      "*"
    ],
    "exclude": [
      "**/*.test.ts"
    ]
  }
}
